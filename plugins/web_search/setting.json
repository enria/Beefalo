{
  "proxy": {
    "http": "http://127.0.0.1:8001"
  },
  "default": {
    "engine": "google",
    "suggestion": "Google"
  },
  "engines": {
    "google": {
      "name": "Google",
      "icon": "images/web_search_google.png",
      "query": "https://www.google.com/search?q={text}",
      "home": "https://www.google.com"
    },
    "bing": {
      "name": "Bing",
      "icon": "images/web_search_bing.png",
      "query": "https://www.bing.com/search?q={text}",
      "home": "https://www.bing.com"
    },
    "youtube": {
      "name": "Youtube",
      "icon": "images/web_search_youtube.png",
      "query": "https://www.youtube.com/results?search_query={text}",
      "home": "https://www.youtube.com"
    },
    "bilibili": {
      "name": "Bilibili",
      "icon": "images/web_search_bilibili.png",
      "query": "https://search.bilibili.com/all?keyword={text}",
      "home": "https://www.bilibili.com/"
    },
    "baidu": {
      "name": "Baidu",
      "icon": "images/web_search_baidu.png",
      "query": "https://www.baidu.com/#ie=UTF-8&wd={text}",
      "home": "https://www.baidu.com"
    },
    "wiki": {
      "name": "Wikipedia",
      "icon": "images/web_search_wiki.png",
      "query": "https://en.wikipedia.org/wiki/{text}",
      "home": "https://en.wikipedia.org/wiki/Main_Page"
    },
    "github": {
      "name": "GitHub",
      "icon": "images/web_search_github3.png",
      "query": "https://github.com/search?q={text}",
      "home": "https://github.com"
    },
    "zhihu": {
      "name": "知乎",
      "icon": "images/web_search_zhihu.png",
      "query": "https://www.zhihu.com/search?type=content&q={text}",
      "home": "https://www.zhihu.com"
    },
    "codelf": {
      "name": "CODELF",
      "icon": "images/web_search_codelf.png",
      "query": "https://unbug.github.io/codelf/#{text}",
      "home": "https://unbug.github.io/codelf"
    },
    "iconfont": {
      "name": "iconfont",
      "icon": "images/web_search_iconfont.png",
      "query": "https://www.iconfont.cn/search/index?searchType=icon&q={text}",
      "home": "https://www.iconfont.cn"
    },
    "flaticon": {
      "name": "flaticon",
      "icon": "images/web_search_flaticon.ico",
